{
  "name": "karomap/php-ogc",
  "description": "GeoSpatial objects (Open Geo Consortium compliant)",
  "license": "GPL-3.0-only",
  "keywords": [
    "geo",
    "geospatial",
    "ogc"
  ],
  "authors": [
    {
      "name": "Lorenzo Giustozzi",
      "email": "giustozzi@lorenzo.im"
    },
    {
      "name": "Rifa'i M. Hanif",
      "email": "hanreev@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.4",
    "creof/wkb-parser": "^2.3",
    "creof/wkt-parser": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Karomap\\PHPOGC\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true
